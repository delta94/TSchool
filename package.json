  "scripts": {
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "start": "ts-node src/index.ts",
    "watch": "ts-node-dev src/index.ts",
    "build-test": "rimraf bin && tsc && rimraf bin",
    "test": "ts-mocha src/**/*.spec.ts",
    "test:watch": "ts-mocha --watch src/**/*.spec.ts",
    "prepush": "npm run build-test && npm run lint && npm test",
    "precommit": "npm run build-test && npm run lint && npm test",
    "clean": "rimraf node_modules ./bin",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:make": "knex migrate:make -x ts $1",
    "seed:make": "knex seed:make -x ts $1",
    "seed:run": "knex seed:run"
  },
